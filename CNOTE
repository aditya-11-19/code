#include <iostream>
using namespace std;

int main() 
{
    int t,x,y,k,n,p,c;
    cin>>t;
	while(t--)
	{
	    cin>>x>>y>>k>>n;
	    x=x-y;
	    int flag=0;
	    while(n--)
	    {
	        cin>>p>>c;
	        if(p>=x&&k>=c)
	        {
	            flag=1;
	        }
	    }
	    if(flag==1)
	    {
	        cout<<"LuckyChef\n";
	    }
	    else
	    cout<<"UnluckyChef\n";
	}
	return 0;
}
